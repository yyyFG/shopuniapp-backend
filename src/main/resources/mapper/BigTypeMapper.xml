<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org/DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace ="cn.demo_text3.mapper.BigTypeMapper">

    <resultMap id="bigTypeResult" type="cn.demo_text3.entity.BigType"></resultMap>
    
    <select id="findById" parameterType="Integer" resultMap="bigTypeResult">
        select * from t_bigtype where id=#{id}
    </select>
    
</mapper>